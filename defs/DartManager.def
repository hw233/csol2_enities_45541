<root>
	<Properties>
	</Properties>

	<BaseMethods>
		<add>									 <!-- 增加镖局相关声望（仇恶）值 -->
			<Arg>   STRING		</Arg>  
			<Arg>	STRING		</Arg>	
			<Arg>	INT32		</Arg>	
		</add>

		<query>									 <!-- 查询镖局相关声望（仇恶）值 -->
			<Arg>   STRING		</Arg>  
			<Arg>   INT8		</Arg>  
			<Arg>	MAILBOX		</Arg>	
		</query>

		<requestPlayerDartPrestige>						<!-- 玩家获取镖局相关声望（仇恶）值 -->
			<Arg>   STRING		</Arg>  
			<Arg>	MAILBOX		</Arg>	
		</requestPlayerDartPrestige>

		<addDartMessage>							<!-- 加入运镖消息 -->
			<Arg>   STRING		</Arg>
			<Arg>	INT32		</Arg>
